    <!-- Query Execution Engine -->
    <property>
        <name>hive.execution.engine</name>
        <value>spark</value>
        <description>
            ‚ö° Query execution engine
            
            Current: spark (fast, in-memory processing)
            
            Other options:
            ‚Ä¢ mr  (MapReduce - slower, stable)
            ‚Ä¢ tez (Optimized DAG execution)
        </description>
    </property>



    <!-- Enable Dynamic Partitioning -->
    <property>
        <name>hive.exec.dynamic.partition</name>
        <value>true</value>
        <description>
            ‚úì Enable dynamic partitioning
            
            Allows automatic partition creation during INSERT
        </description>
    </property>

    <!-- Dynamic Partition Mode -->
    <property>
        <name>hive.exec.dynamic.partition.mode</name>
        <value>nonstrict</value>
        <description>
            üîì Dynamic partition mode
            
            Current: nonstrict (all partitions can be dynamic)
            
            Options:
            ‚Ä¢ strict:    At least one static partition required
            ‚Ä¢ nonstrict: All partitions can be dynamic
        </description>
    </property>

    <!-- Max Dynamic Partitions -->

    <!-- Connection URL -->
    <property>
        <name>javax.jdo.option.ConnectionURL</name>
        <value>jdbc:derby:;databaseName=/tmp/metastore_db;create=true</value>
        <description>
            üìç JDBC connection URL for the Derby metastore database
            
            Current: Derby embedded (local development)
            Location: /tmp/metastore_db
            
            Production alternatives:
            ‚Ä¢ MySQL:      jdbc:mysql://localhost:3306/metastore
            ‚Ä¢ PostgreSQL: jdbc:postgresql://localhost:5432/metastore
        </description>
    </property>

    <!-- Driver Class -->
    <property>
        <name>javax.jdo.option.ConnectionDriverName</name>
        <value>org.apache.derby.jdbc.EmbeddedDriver</value>
        <description>
            üîå JDBC driver class for Derby embedded database
        </description>
    </property>

    <!-- Database Credentials -->
    <property>
        <name>javax.jdo.option.ConnectionUserName</name>
        <value>app</value>
        <description>üîê Database username (Derby default)</description>
    </property>

    <property>
        <name>javax.jdo.option.ConnectionPassword</name>
        <value>mine</value>
        <description>üîê Database password (Derby default)</description>
    </property>

    <!-- Metastore URI (Empty = Embedded) -->
    <property>
        <name>hive.metastore.uris</name>
        <value></value>
        <description>
            üåê Metastore service URI
            
            Current: Empty (embedded metastore)
            Remote:  thrift://localhost:9083
        </description>
    </property>


